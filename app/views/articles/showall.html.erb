<pre>
  <% @articles.each do |article| %>
    @article{<%= article.key %>,
      author = {<%= article.author %>},
      title = {<%= article.title %>},
      journal = {<%= article.journal %>},
      year = {<%= article.year %>},
    }

  <% end %>
  <% @books.each do |book| %>
    @book{<%= book.key %>,
      author = {<%= book.author %>},
      title = {<%= book.title %>},
      publisher = {<%= book.publisher %>},
      year = {<%= book.year %>},
    }

  <% end %>
  <% @inproceedings.each do |ip| %>
    @inproceeding{<%= ip.key %>,
      author = {<%= ip.author %>},
      title = {<%= ip.title %>},
      booktitle = {<%= ip.booktitle %>},
      year = {<%= ip.year %>},
    }

  <% end %>
</pre>